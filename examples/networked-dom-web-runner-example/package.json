{
  "name": "networked-dom-web-runner-example",
  "version": "0.13.0",
  "private": true,
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsx ./build.ts --build",
    "iterate": "cross-env PORT=6002 tsx ./build.ts --serve",
    "serve": "node --enable-source-maps ./build/index.js",
    "lint": "eslint \"./{src,test}/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint-fix": "eslint \"./{src,test}/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {
    "@mml-io/networked-dom-web-runner": "^0.13.0"
  },
  "devDependencies": {
    "esbuild-css-modules-plugin": "3.1.1"
  }
}
