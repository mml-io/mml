                <xs:enumeration value="easeInQuint"/>
                <xs:enumeration value="easeOutQuint"/>
                <xs:enumeration value="easeInOutQuint"/>
                <xs:enumeration value="easeInSine"/>
                <xs:enumeration value="easeOutSine"/>
