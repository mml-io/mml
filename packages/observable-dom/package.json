{
  "name": "@mml-io/observable-dom",
  "version": "0.24.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "type": "module",
  "files": [
    "/build"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
