{
  "name": "@mml-io/mml-web-client",
  "version": "0.18.1",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/build"
  ],
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsx ./build.ts --build",
    "iterate": "cross-env PORT=28891 tsx ./build.ts --serve",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint-fix": "eslint \"./**/*.{js,jsx,ts,tsx}\" --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test-iterate": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "dependencies": {
    "@mml-io/mml-web": "^0.18.1",
    "three": "0.163.0",
    "playcanvas": "1.73.5",
    "@mml-io/mml-web-playcanvas-standalone": "^0.18.1",
    "@mml-io/mml-web-threejs-standalone": "^0.18.1"
  },
  "devDependencies": {
    "@types/three": "0.163.0"
  }
}
