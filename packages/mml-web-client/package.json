{
  "name": "mml-web-client",
  "version": "0.9.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/build"
  ],
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsx ./build.ts --build",
    "iterate": "cross-env PORT=28891 tsx ./build.ts --serve",
    "lint": "eslint \"./{src,test}/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint-fix": "eslint \"./{src,test}/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {
    "mml-web": "^0.9.0",
    "three": "0.153.0"
  },
  "devDependencies": {
    "@types/three": "0.152.1"
  }
}
