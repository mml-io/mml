{
  "name": "mml-web-client",
  "version": "0.4.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/build"
  ],
  "main": "build/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "node ./build.js --build",
    "iterate": "cross-env PORT=28891 node ./build.js --serve",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint-fix": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {
    "mml-web": "^0.4.0",
    "three": "0.153.0"
  },
  "devDependencies": {
    "@types/three": "0.152.1"
  }
}
